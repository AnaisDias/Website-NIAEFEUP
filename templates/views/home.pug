extends ../layouts/default

block content
    section#hero.hero
        .item.center-block
            .slide-info.center-block
                p.title Encontro Nacional de Estudantes de Informática
                hr.white-separator
                p.subtitle Se és estudante de Engenharia Informática, não podes perder este evento!
                button.btn.btn-default.see-more-button Ver Mais
    section#info 
        .row
          .col-md-10.col-md-offset-1.text-center
            h1.subtitle Associação de Estudantes da Faculdade de Engenharia da Universidade do Porto
            h1.title NÚCLEO DE INFORMÁTICA
            h6.definition Nós somos um núcleo da Associação de Estudantes da FEUP, constituído inteiramente por alunos do curso de Engenharia Informática e Computação (MIEIC). A nossa principal motivação é ganharmos experiência e competências que a organização de eventos e projetos relacionados com o núcleo nos exige.
            hr.black-separator   
